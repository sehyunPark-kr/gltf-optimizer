{
  "output_ext" : [".gltf", ".glb"],
  "texture_settings" : [
    {
      "max_size" : 128,

      "uastc_keywords" : ["final"],
  
      "uastc_quality" : 2,
      "uastc_rdo_l": 0.5,
      "uastc_rdo_d": 8192,
      "zcmp": 22,
      "assign_oetf": "srgb",
      "assign_primaries": "none"
    }
  ],
  "model_settings" : [
    {
      "suffix" : "_LOD0"
    },
    {
      "suffix" : "_LOD1",
      "tolerance" : 0.0001,
      "ratio" : 0.5,
      "error" : 0.01
    }
  ]
}
